    },
    {
      "scope": "/media/ai-generated/*",
      "content_types": ["images", "video"],
      "ats_tier": "ATS-4",
      "ats_extent": "E4",
      "deepfake_disclosure": true,
      "human_oversight": true,
      "c2pa_status": "signed",
      "description": "AI-generated media assets â€” deepfake disclosure per Art. 50 Abs. 4."
    },
    {
      "scope": ["/impressum", "/agb", "/datenschutz", "/karriere/*"],
      "ats_tier": "ATS-0",
      "ats_extent": "E0",
      "description": "Strictly human-authored. No AI disclosure required."
    }
  ],
  "transparency_policy_url": "https://enterprise.example.com/ai-transparency",
